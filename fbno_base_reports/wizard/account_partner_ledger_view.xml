<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_report_partner_ledger_view_summary" model="ir.ui.view">
        <field name="name">Partner Ledger Summary</field>
        <field name="model">account.report.partner.ledger</field>
        <field name="inherit_id" ref="base_accounting_kit.account_report_partner_ledger_view"/>
        <field name="arch" type="xml">
        <data>
            <xpath expr="//field[@name='date_from']" position="before">
                <field name="summary_report"/>
            </xpath>
        </data>
        </field>
    </record>
</odoo>
